<T3DataStructure>
    <sheets>
        <sDEF>
            <ROOT>
                <el>
                    <settings.assistantId>
                        <TCEforms>
                            <label>Assistant ID</label>
                            <config>
                                <type>select</type>
                                <renderType>selectSingle</renderType>
                                <itemsProcFunc>Effective\Aiassistant\Controller\AssistantController->fillAssistants</itemsProcFunc>
                                <size>1</size>
                                <maxitems>1</maxitems>
                                <minitems>0</minitems>
                            </config>
                        </TCEforms>
                    </settings.assistantId>
                    <settings.pluginType>
                        <TCEforms>
                            <label>Plugin type (full size or minified chat)</label>
                            <config>
                                <type>select</type>
                                <renderType>selectSingle</renderType>
                                <items type="array">
                                    <numIndex index="0" type="array">
                                        <numIndex index="0">Full size chat</numIndex>
                                        <numIndex index="1">fullsize</numIndex>
                                    </numIndex>
                                    <numIndex index="1" type="array">
                                        <numIndex index="0">Minified chat</numIndex>
                                        <numIndex index="1">minified</numIndex>
                                    </numIndex>
                                </items>
                            </config>
                        </TCEforms>
                    </settings.pluginType>
                </el>
            </ROOT>
        </sDEF>
    </sheets>
</T3DataStructure>
